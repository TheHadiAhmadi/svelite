{
	"name": "svelitecms",
	"version": "0.0.2-next.41",
	"bin": {
		"svelite": "./bin/svelite.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"exports": {
		"./components": {
			"types": "./dist/core/index.d.ts",
			"svelte": "./dist/core/index.js"
		},
		"./admin": {
			"import": "./dist/admin/plugin.js"
		},
		"./config/svelte": {
			"import": "./dist/config/svelte.config.js"
		},
		"./config/vite": {
			"import": "./dist/config/vite.config.js"
		},
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"svelte": "./dist/index.js"
		},
		"./styles.css": {
			"import": "./dist/styles.css"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-auto": "^3.1.0",
		"@sveltejs/adapter-node": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.3.2",
		"@sveltejs/package": "^2.2.5",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"iconify-icon": "^1.0.8",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.11"
	},
	"peerDependencies": {
		"svelte": "^5.0.0-next.34"
	}
}
